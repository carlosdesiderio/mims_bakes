<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="BrownHeadline" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textColor">@color/teal_800</item>
    </style>
    <style name="TealSubhead" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">@color/teal_800</item>
    </style>

    <style name="AppTheme.ProgressBarCustomized" parent="@style/Theme.AppCompat">
        <item name="colorAccent">#fff</item>
    </style>

</resources>
